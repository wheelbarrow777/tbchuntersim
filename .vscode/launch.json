    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "run",
                "-i",
                "1",
                "--log-level",
                "DEBUG",
                "-f",
                "presets/wip.json"
            ]
        },
        {
            "name": "Debug Generate",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "preset",
                "generate",
                "mynewcoolfile.json",
            ]
        }
    ]
}